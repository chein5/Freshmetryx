        android:textAlignment="center"
        android:textColor="#02060D"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="131dp"
        android:layout_height="31dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="52dp"
        android:text="FINANZAS"
        android:textAlignment="center"
        android:textColor="#02060D"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/btnInventario"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="82dp"
        android:layout_height="64dp"
        android:layout_marginStart="3dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton9"
        app:srcCompat="@drawable/inicio_icon" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="81dp"
        android:layout_height="64dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/perfil_icon" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="84dp"
        android:layout_height="65dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton8"
        app:srcCompat="@drawable/venta_icon" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="82dp"
        android:layout_height="65dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton11"
        app:srcCompat="@drawable/cuenta_icon" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="78dp"
        android:layout_height="68dp"
        android:layout_marginEnd="4dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/pedido_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>